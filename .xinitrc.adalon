xset +fp /usr/local/lib/X11/fonts/terminus
xset fp rehash

xset -b
xrdb -merge ~/.Xresources
setxkbmap gb
xscreensaver -no-splash &
dbus-daemon --session --fork
source ~/.bashrc

export LANG="en_US.UTF-8"
export QT_X11_NO_MITSHM=1
export LC_CTYPE=$LANG
export LC=$LANG

#startfluxbox
exec ratpoison

